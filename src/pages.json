{
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/engine/default",
			"style": {
				"navigationBarTitleText": "loading"
			}
		},
		{
			"path": "pages/engine/custom",
			"style": {
				"navigationBarTitleText": "loading",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/test/index",
			"style": {
				"navigationBarTitleText": "测试页"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"background": "#efeff4"
		},
		"usingComponents": {
			"render": "components/Render/index"
		}
	},
	"easycom": {
	    "autoscan": true,
	    "custom": {
	      "^nut-(.*)?-(.*)": "nutui-uniapp/components/$1$2/$1$2.vue",
	      "^nut-(.*)": "nutui-uniapp/components/$1/$1.vue"
	    }
	},
	"tabBar": {
		"color": "#7D7E80",
		"selectedColor": "#fa2c19",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页",
			"iconPath": "static/images/home.png",
			"selectedIconPath": "static/images/selected_home.png"
		}, {
			"pagePath": "pages/my/index",
			"text": "我的",
			"iconPath": "static/images/my.png",
			"selectedIconPath": "static/images/selected_my.png"
		}]
	}
}
